split_bootimg.pl recovery.imgmkdir ramdiskcd ramdiskgzip -dc ../recovery.img-ramdisk.gz | cpio -icd ..mkbootfs ./ramdisk | gzip > ramdisk-new.gzmkbootimg --cmdline 'no_console_suspend=1 console=null' --kernel recovery.img-kernel --ramdisk ramdisk-new.gz -o recovery-new.imgsplit_bootimg.pl boot.imgmkdir ramdiskcd ramdiskgzip -dc ../boot.img-ramdisk.gz | cpio -icd ..mkbootfs ./ramdisk | gzip > ramdisk-new.gzmkbootimg --cmdline 'no_console_suspend=1 console=null' --kernel boot.img-kernel --ramdisk ramdisk-new.gz -o boot-new.imgmake -j4 otatoolsexport PATH=$PATH:~/cm10.1/out/host/linux-x86/binbuild/tools/device/mkvendor.sh htc protodcg ~/recovery.img. build/envsetup.sh. build/tools/device/makerecoveries.sh cm_protodcg-eng# Android environmentexport TARGET_ARCH=armexport ANDROID_SDK_TOOLS=/usr/android-sdk/toolsexport ANDROID_SDK_ADB=/usr/android-sdk/platform-toolsexport ANDROID_TOOLCHAIN=~/cm10.1/prebuilts/gcc/linux-x86/arm/arm-eabi-4.6export PATH=${ANDROID_SDK_TOOLS}:${ANDROID_TOOLCHAIN}:${ANDROID_SDK_ADB}:$PATH# Android environmentexport TARGET_ARCH=armexport ANDROID_SDK_TOOLS=/usr/android-sdk/toolsexport ANDROID_SDK_ADB=/usr/android-sdk/platform-toolsexport ANDROID_TOOLCHAIN=~/cm10.1/prebuilt/linux-x86/toolchain/arm-eabi-4.4.3export PATH=${ANDROID_SDK_TOOLS}:${ANDROID_TOOLCHAIN}:${ANDROID_SDK_ADB}:$PATHmake -j32 otatoolsbuild/tools/device/mkvendor.sh htc prototd ~/recovery.img. build/envsetup.shlunch cm_prototd-engmake -j32 recoveryimagemake installcleancd device/htc/protodcg./extract-files.shvendor/cm/get-prebuiltsmake -j16